public class NumericCommand
{
 public static void main(String args[])
 {
	int firstarg;
	if (args.length> 0)
	{
	 try
	 {
		firstarg = Integer.parseInt(args[0]);
	 } 
	 catch (NumberFormatException e) 
	 {
		System.err.println("Argument" + args[0] + " must be an integer.");
		System.exit(1);
	 }
	}
 }
}
