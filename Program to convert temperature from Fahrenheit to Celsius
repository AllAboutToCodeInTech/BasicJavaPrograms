import java.util.*;
class FaherenheitCelsius
{
 public static void main(String[] args) 
 {
  double Faherenheit,Celsius;  
  System.out.println("Enter temperature in Faherenheit:");
  Scanner sc=new Scanner(System.in);
  Faherenheit = sc.nextDouble();
  Celsius = (double)((5*(Faherenheit-32)) /9);
  System.out.println("Temperature in Celsius is:â€+Celsius);
 }
}
